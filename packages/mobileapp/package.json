{
  "name": "@covid19-saviors/mobileapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "clean": "rm -rf node_modules",
    "prettier:base": "prettier --single-quote",
    "prettier:check": "yarn prettier:base --list-different \"**/*.+(js|jsx|json|md)\"",
    "prettier:fix": "yarn prettier:base --write \"**/*.+(js|jsx|json|md)\"",
    "lint:js": "eslint **/*.js",
    "lint:fix": "yarn lint:js --fix",
    "test": "jest --coverage --passWithNoTests",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,css,md}": [
      "yarn prettier:fix",
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@covid19-saviors/common": "1.0.0",
    "@ui-kitten/eva-icons": "^5.0.0-alpha.1",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-maps": "0.27.1",
    "react-native-paper": "^3.8.0",
    "react-native-vector-icons": "^6.6.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.11.0"
  }
}
